          <argument>
            <kind>1</kind>
            <string>/// read check the timeline

// set all variable to false
cmd_stall = false;
cmd_blast = false;
cmd_jump = false;
cmd_wall_kick = false;

var fpm = bpm*fpb;

if (counter &gt;= fpm)
    counter = counter % fpm;

// check for all actions (before this beat that haven't been processed yet)
for (index = 0; index &lt; array_length_1d(sequence); index += 1)
{
    var pair = sequence[index];
    var time = pair[0];
    var action = pair[1];
    if (((counter - 1)/fpb &lt;= time &amp;&amp; counter / fpb &gt; time)
    || ((counter + fpm - 1)/fpb &lt;= time &amp;&amp; (counter + fpm) &gt; time))
    {
        switch(action)
        {
        case command.stall:
            cmd_stall = true;
            break;
        case command.blast:
            cmd_blast = true;
            break;
        case command.jump:
            cmd_jump = true;
            break;
        case command.wall_kick:
            cmd_wall_kick = true;
            break;
        default:    // none
            break;
        }
    }
}

kill_player = mark_kill_player;
mark_kill_player = false;

// increment the counter
if (beat_id != 0)
{
    tick_beat = counter % fpb == 0;
    down_beat = counter == 0;
    counter += 1;
}
</string>
