            <kind>1</kind>
            <string>/// variable initialization

randomize();    // random variables enabled

show_debug_message("manager Create");

audio_stop_all();

// alter in obj_manager creation code if necessary
bpm = 4;            // beats per measure
fpb = 30;           // frames per beat
music = electroswing120bpm;

// constants!
enum command
{
    jump = 0,
    swipe,
    plat_move,
    
    trigger,    // situation dependent
    
    none
}

//speed up
is_fast = false;

// modify me
sequence[0] = Pair(0,command.none);
beat_id = 0;
checkpoint_id = 0;
mark_kill_player = false;

// per beat
tick_beat = false;  // every beat
down_beat = false;  // first beat of the measure

command_bitmask = 0;
command_tripple_map = ds_map_create();

kill_player = false;


// counter
counter = 0;


// create other components
instance_create(0,0,obj_audio);
instance_create(0,0,obj_trigger_manager);


//timing for k=obj_kill

isStart = false;
step_cnt = -1;
cur_bit = 0;
cur_bit_start = 0;
cur_bit_end = fpb - 1; // included
</string>
          </argument>
