        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// read check the timeline

var fpm = bpm*fpb;

if (counter &gt;= fpm)
    counter = counter % fpm;
    
command_bitmask = 0;
ds_map_clear(command_tripple_map);
// check for all actions (before this beat that haven't been processed yet)
for (index = 0; index &lt; array_length_1d(sequence); index += 1)
{
    var pair = sequence[index];
    var time = pair[0];
    var action = pair[1];
    
    if (((counter - 1)/fpb &lt;= time &amp;&amp; counter/fpb &gt; time)
    || ((counter + fpm - 1)/fpb &lt;= time &amp;&amp; (counter + fpm)/fpb &gt; time))
    {   
        command_bitmask = command_bitmask | (1 &lt;&lt; action);
        if (array_length_1d(pair) == 3)
        {
            var extra = pair[2];
            ds_map_add(command_tripple_map,action,extra);
        }
    }
}

kill_player = mark_kill_player;
mark_kill_player = false;

// increment the counter
if (beat_id != 0)
{
    tick_beat = counter % fpb == 0;
    down_beat = counter == 0;
    counter += 1;
}
</string>
