          <argument>
            <kind>1</kind>
            <string>///timing

step_cnt = (step_cnt + 1) % (bpm*fpb);

if (step_cnt % fpb == 0){
    cur_bit = step_cnt / fpb;
    cur_bit_start = step_cnt;
    cur_bit_end = cur_bit_start + fpb -1;
}



